# Environment Variables Template
# Copy this file to .env and fill in your values

# Private Keys - NEVER commit these to git!
PRIVATE_KEY=your_private_key_here
PRIVATE_KEY_PASSWORD=your_password_here

# Account Name for Encrypted Keys
ACCOUNT_NAME=defaultKey

# Network RPC URLs
ETH_RPC_URL=https://ethereum-rpc-url
ZKSYNC_RPC_URL=https://sepolia.era.zksync.dev

# Account for Foundry
ACCOUNT=defaultKey
SENDER=0x_your_address_here

# API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key

# Contract Addresses (fill these after deployment)
ZK_MINIMAL_ADDRESS=0x_your_deployed_zk_minimal_account_address
